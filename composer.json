{
  "name": "pollen-solutions/wp-salt",
  "description": "Generateur de cl√© de Hashage Wordpress.",
  "homepage": "https://github.com/milkcreation/pollen-wp-salt.git",
  "keywords": [
    "pollen-solutions",
    "component",
    "wordpress",
    "wp-salt"
  ],
  "time": "2021-09-15",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0 || ^8.0",
    "composer/installers": "~1.0",
    "laminas/laminas-math": "^3.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\WpSalt\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\WpSalt\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "bin": [
    "bin/wp-salt"
  ]
}
