{
  "name": "pollen-solutions/wp-salt",
  "description": "Generateur de cl√© de Hashage Wordpress.",
  "homepage": "https://pollen-solutions/components/wp-salt/",
  "keywords": [
    "pollen-solutions",
    "component",
    "wordpress",
    "wp-salt"
  ],
  "time": "2020-12-12",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy.manner@milkcreation.fr",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.tigreblanc.fr/pollen-solutions/"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "composer/installers": "~1.0",
    "laminas/laminas-math": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\WpSalt\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\WpSalt\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "bin": [
    "bin/wp-salt"
  ]
}
